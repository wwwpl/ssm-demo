<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
		"http://ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="PeopleMapple">
	
	<typeAlias alias="People" type="com.taru.project.model.People"/>

	<resultMap id="PeopleModel" class="People">
		<result property="name" column="name"/>
		<result property="age" column="age"/>
	</resultMap>

	<select id="getList" resultMap="PeopleModel">
		select * from people
	</select>
</sqlMap>